{
  "name": "first-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "prisma generate && next dev",
    "clean": "rimraf .next",
    "deep-clean": "rimraf node_modules .next .prisma",
    "build": "prisma generate && next build",
    "start": "next start",
    "start-socket": "node server/socket-server.js",
    "start-all": "concurrently \"npm run start\" \"npm run start-socket\"",
    "lint": "eslint",
    "postinstall": "rimraf ./node_modules/.prisma && prisma generate"
  },
  "dependencies": {
    "bcryptjs": "^3.0.3",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.15.3",
    "next": "16.0.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@prisma/client": "^6.19.0",
    "@tailwindcss/postcss": "^4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "prisma": "^6.19.0",
    "rimraf": "^5.0.7",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
